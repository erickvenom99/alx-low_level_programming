#include "dog.h"
/**
 * init_dog - Short description
 * @d: A pointer to struct
 * @name: Second member
 * @age:member of struct dog;
 * @owner: member of struct dog
 *
 * Description: Longer description
 */
void init_dog(struct dog *d, char *name, float age, char *owner)
{
	(*d).name = name;
	(*d).age = age;
	(*d).owner = owner;
}
